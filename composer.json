{
  "name": "tourze/shadowsocksr-config-php",
  "description": "ShadowsocksR Config",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "tourze/shadowsocks-config-php": "0.0.*",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "shipmonk/composer-dependency-analyser": "^1.8"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "ShadowsocksR\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ShadowsocksR\\Config\\Tests\\": "tests"
    }
  }
}